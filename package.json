{
  "name": "educationandcare",
  "version": "1.0.0",
  "description": "Education and care website",
  "main": "index.js",
  "scripts": {
    "build": "npm run sass && npm run imagemin && eleventy",
    "imagemin": "imagemin src/images/raw/* --out-dir=src/images/min",
    "sass": "sass --style=compressed src/_sass/style.scss src/_includes/css/style.css",
    "start": "npm-run-all sass --parallel watch:*",
    "watch:eleventy": "eleventy --serve",
    "watch:sass": "npm run sass -- --watch"
  },
  "keywords": [],
  "author": "Ben White",
  "license": "Apache-2.0",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-cache-assets": "^2.0.3",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "eleventy-plugin-seo": "^0.5.1",
    "imagemin-cli": "^6.0.0",
    "sass": "^1.32.8"
  }
}
